--// 11. INFINITE JUMP HANDLER
UserInputService.JumpRequest:Connect(function()
    if _G.YslSettings.Jump and humanoid then
        pcall(function()
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end)
    end
end)

--// 12. CHARACTER RESPAWN HANDLER
lplayer.CharacterAdded:Connect(function(newCharacter)
    task.wait(1)
    character = newCharacter
    humanoid = newCharacter:WaitForChild("Humanoid")
    rootPart = newCharacter:WaitForChild("HumanoidRootPart")
    
    -- Reapply all active features
    if _G.YslSettings.WaveImmunity then
        setupUltimateWaveImmunity()
    end
    if _G.YslSettings.VIPBypass then
        setupVIPDoorBypass()
    end
    if _G.YslSettings.Speeding then
        setupSpeedBypass()
    end
    if _G.YslSettings.NoClip then
        setupNoClip()
    end
    if _G.YslSettings.Magnet then
        setupSmartMagnet()
    end
    if _G.YslSettings.AutoCollect then
        setupAutoCollect()
    end
    
    Rayfield:Notify({
        Title = "Character Respawned",
        Content = "All features reapplied âœ…",
        Duration = 3
    })
end)

