--// AUTO USE SPAWN MACHINE
local function SetupAutoSpawnMachine()
    if not _G.YslBrainrot.AutoUseSpawnMachine then return end
    
    task.spawn(function()
        while _G.YslBrainrot.AutoUseSpawnMachine do
            pcall(function()
                -- Find spawn machine
                local spawnMachine = Workspace:FindFirstChild("SpawnMachine") or
                                    Workspace:FindFirstChild("LuckMachine")
                
                if spawnMachine then
                    -- Check if luck bar is full (implement your own detection)
                    local clickDetector = spawnMachine:FindFirstChildOfClass("ClickDetector", true)
                    if clickDetector then
                        fireclickdetector(clickDetector)
                        task.wait(5)
                    end
                end
            end)
            task.wait(10)
        end
    end)
end

--// REMOVE ALL WAVES
local function RemoveAllWaves()
    local count = 0
    for _, v in pairs(Workspace:GetDescendants()) do
        if v:IsA("BasePart") or v:IsA("Model") then
            local name = v.Name:lower()
            if name:find("wave") or name:find("tsunami") or name:find("water") or name:find("flood") then
                -- Destroy scripts first
                for _, script in pairs(v:GetDescendants()) do
                    if script:IsA("Script") or script:IsA("LocalScript") then
                        script:Destroy()
                    end
                end
                v:Destroy()
                count = count + 1
            end
        end
    end
    
    -- Clear terrain water
    Workspace.Terrain:Clear()
    
    return count
end

--// ESP SYSTEMS
local function CreateESP(part, color, text)
    if part:FindFirstChild("YslESP") then return end
    
    local highlight = Instance.new("Highlight")
    highlight.Name = "YslESP"
    highlight.Parent = part
    highlight.FillColor = color
    highlight.OutlineColor = color
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
    
    local billboard = Instance.new("BillboardGui")
    billboard.Name = "YslESP"
    billboard.Parent = part
    billboard.Size = UDim2.new(0, 100, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true
    
    local label = Instance.new("TextLabel")
    label.Parent = billboard
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = color
    label.TextScaled = true
    label.Font = Enum.Font.GothamBold
    label.TextStrokeTransparency = 0.5
end

local function SetupBrainrotESP()
    if not _G.YslBrainrot.BrainrotESP then
        for _, v in pairs(Workspace:GetDescendants()) do
            if v.Name == "YslESP" then v:Destroy() end
        end
        return
    end
    
    task.spawn(function()
        while _G.YslBrainrot.BrainrotESP do
            pcall(function()
                local folder = GetBrainrotFolder()
                for _, brainrot in pairs(folder:GetChildren()) do
                    if brainrot:IsA("Model") then
                        local part = brainrot.PrimaryPart or brainrot:FindFirstChildWhichIsA("BasePart")
                        if part and not part:FindFirstChild("YslESP") then
                            local color = Color3.fromRGB(255, 255, 0)
                            
                            -- Color code by rarity
                            local name = brainrot.Name:lower()
                            if name:find("divine") then
                                color = Color3.fromRGB(255, 0, 255)
                            elseif name:find("celestial") then
                                color = Color3.fromRGB(0, 255, 255)
                            elseif name:find("cosmic") then
                                color = Color3.fromRGB(138, 43, 226)
                            elseif name:find("legendary") then
                                color = Color3.fromRGB(255, 165, 0)
                            end
                            
                            CreateESP(part, color, brainrot.Name)
                        end
                    end
                end
            end)
            task.wait(2)
        end
    end)
end

local function SetupSafezoneESP()
    if not _G.YslBrainrot.SafezoneESP then
        for _, v in pairs(Workspace:GetDescendants()) do
            if v.Name == "YslESP" then v:Destroy() end
        end
        return
    end
    
    local safezones = GetSafeZones()
    for _, zone in pairs(safezones) do
        CreateESP(zone, Color3.fromRGB(0, 255, 0), "SAFE")
    end
end

local function SetupUFOCoinESP()
    if not _G.YslBrainrot.UFOCoinESP then
        for _, v in pairs(Workspace:GetDescendants()) do
            if v.Name == "YslESP" then v:Destroy() end
        end
        return
    end
    
    task.spawn(function()
        while _G.YslBrainrot.UFOCoinESP do
            pcall(function()
                for _, v in pairs(Workspace:GetDescendants()) do
                    if v:IsA("BasePart") and v.Name:lower():find("ufo") and v.Name:lower():find("coin") then
                        if not v:FindFirstChild("YslESP") then
                            CreateESP(v, Color3.fromRGB(255, 0, 255), "UFO COIN")
                        end
                    end
                end
            end)
            task.wait(2)
        end
    end)
end

local function SetupDivineLuckyBlockESP()
    if not _G.YslBrainrot.DivineLuckyBlockESP then
        for _, v in pairs(Workspace:GetDescendants()) do
            if v.Name == "YslESP" then v:Destroy() end
        end
        return
    end
    
    task.spawn(function()
        while _G.YslBrainrot.DivineLuckyBlockESP do
            pcall(function()
                for _, v in pairs(Workspace:GetDescendants()) do
                    if v:IsA("Model") or v:IsA("BasePart") then
                        if v.Name:lower():find("divine") and v.Name:lower():find("lucky") then
                            local part = v:IsA("Model") and (v.PrimaryPart or v:FindFirstChildWhichIsA("BasePart")) or v
                            if part and not part:FindFirstChild("YslESP") then
                                CreateESP(part, Color3.fromRGB(255, 0, 255), "DIVINE BLOCK")
                            end
                        end
                    end
                end
            end)
            task.wait(2)
        end
    end)
end

