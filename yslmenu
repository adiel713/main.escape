local success, Rayfield = pcall(function()
    return loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
end)

if not success then
    warn("Failed to load Rayfield library")
    return
end

--// Services
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

--// Variables
local lplayer = Players.LocalPlayer
local character = lplayer.Character or lplayer.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local rootPart = character:WaitForChild("HumanoidRootPart")

--// Global Settings
_G.YslSettings = {
    -- Movement
    Speeding = false,
    Jump = false,
    SpeedValue = 100,
    NoClip = false,
    
    -- Collection
    Magnet = false,
    MagnetSpeed = 0.03,
    AutoCollect = false,
    
    -- Protection
    WaveImmunity = false,
    GodMode = false,
    AntiDrown = false,
    AntiKill = false,
    
    -- Bypass
    VIPBypass = false,
    DoorBypass = false,
    
    -- Stats
    CoinsCollected = 0,
    WavesBlocked = 0
}

--// Anti-Cheat Bypass
local function setHumanoidProperty(property, value)
    pcall(function()
        if humanoid then
            humanoid[property] = value
        end
    end)
end

--// ULTIMATE Wave Immunity System (Actually prevents death)
local waveImmunityConnection
local function setupUltimateWaveImmunity()
    if waveImmunityConnection then
        waveImmunityConnection:Disconnect()
    end
    
    if not _G.YslSettings.WaveImmunity then return end
    
    -- Method 1: Disable all wave damage scripts
    pcall(function()
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("BasePart") then
                local name = v.Name:lower()
                if name:find("wave") or name:find("water") or name:find("tsunami") or 
                   name:find("flood") or name:find("kill") or name:find("damage") then
                    
                    -- Destroy all scripts that could cause damage
                    for _, script in pairs(v:GetDescendants()) do
                        if script:IsA("Script") or script:IsA("LocalScript") then
                            script.Enabled = false
                            script:Destroy()
                        end
                    end
                    
                    -- Disable all touch events
                    v.CanTouch = false
                    v.CanCollide = false
                    
                    -- Remove from collision group
                    pcall(function()
                        v.CollisionGroup = "NoCollision"
                    end)
                end
            end
        end
    end)
    
    -- Method 2: Hook all damage to player
    waveImmunityConnection = humanoid:GetPropertyChangedSignal("Health"):Connect(function()
        if _G.YslSettings.WaveImmunity or _G.YslSettings.GodMode then
            if humanoid.Health < humanoid.MaxHealth then
                humanoid.Health = humanoid.MaxHealth
                _G.YslSettings.WavesBlocked = _G.YslSettings.WavesBlocked + 1
            end
        end
    end)
    
    -- Method 3: Prevent damage from touching water
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            local connections = getconnections(part.Touched)
            for _, connection in pairs(connections) do
                if _G.YslSettings.WaveImmunity then
                    connection:Disable()
                else
                    connection:Enable()
                end
            end
        end
    end
    
    -- Method 4: Force humanoid state to prevent drowning
    humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, false)
    humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
    
    -- Method 5: Remove water from terrain constantly
    task.spawn(function()
        while _G.YslSettings.WaveImmunity do
            pcall(function()
                local region = Region3.new(
                    rootPart.Position - Vector3.new(50, 50, 50),
                    rootPart.Position + Vector3.new(50, 50, 50)
                )
                Workspace.Terrain:FillRegion(region, 4, Enum.Material.Air)
            end)
            task.wait(0.1)
        end
    end)
end

--// VIP Door Bypass System
local doorBypassConnection
local function setupVIPDoorBypass()
    if doorBypassConnection then
        doorBypassConnection:Disconnect()
    end
    
    if not _G.YslSettings.VIPBypass then return end
    
    -- Method 1: Remove all door barriers
    pcall(function()
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("BasePart") then
                local name = v.Name:lower()
                local parent = v.Parent and v.Parent.Name:lower() or ""
                
                -- Check if it's a VIP/locked door
                if name:find("vip") or name:find("door") or name:find("barrier") or 
                   name:find("gate") or name:find("lock") or parent:find("vip") or 
                   parent:find("door") then
                    
                    -- Make door intangible
                    v.CanCollide = false
                    v.CanTouch = false
                    v.Transparency = 0.8
                    
                    -- Remove ProximityPrompts (VIP prompts)
                    for _, prompt in pairs(v:GetDescendants()) do
                        if prompt:IsA("ProximityPrompt") then
                            prompt.Enabled = false
                        end
                    end
                    
                    -- Destroy any door scripts
                    for _, script in pairs(v:GetDescendants()) do
                        if script:IsA("Script") or script:IsA("LocalScript") then
                            if script.Name:lower():find("door") or script.Name:lower():find("vip") then
                                script:Destroy()
                            end
                        end
                    end
                end
            end
        end
    end)
    
    -- Method 2: Auto-unlock doors on approach
    doorBypassConnection = RunService.Heartbeat:Connect(function()
        if not _G.YslSettings.VIPBypass then return end
        
        pcall(function()
            local nearbyDoors = {}
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") and (v.Name:lower():find("door") or v.Name:lower():find("vip")) then
                    local distance = (v.Position - rootPart.Position).Magnitude
                    if distance < 30 then
                        v.CanCollide = false
                        
                        -- Open door if it has a configuration
                        if v.Parent:FindFirstChild("Configuration") then
                            v.Parent.Configuration.Locked.Value = false
                        end
                    end
                end
            end
        end)
    end)
end

--// Enhanced Speed Bypass (Multi-method)
local speedConnection
local function setupSpeedBypass()
    if speedConnection then
        speedConnection:Disconnect()
    end
    
    if not _G.YslSettings.Speeding then return end
    
    speedConnection = RunService.Heartbeat:Connect(function()
        pcall(function()
            if not humanoid or not rootPart then return end
            
            -- Method 1: Direct WalkSpeed
            humanoid.WalkSpeed = _G.YslSettings.SpeedValue
            
            -- Method 2: CFrame movement boost
            if humanoid.MoveDirection.Magnitude > 0 then
                local moveDirection = humanoid.MoveDirection
                local speedMultiplier = (_G.YslSettings.SpeedValue / 16) - 1
                
                rootPart.CFrame = rootPart.CFrame + (moveDirection * speedMultiplier * 0.1)
            end
            
            -- Method 3: Velocity boost
            if humanoid.MoveDirection.Magnitude > 0 then
                local velocity = rootPart.Velocity
                velocity = Vector3.new(
                    humanoid.MoveDirection.X * _G.YslSettings.SpeedValue,
                    velocity.Y,
                    humanoid.MoveDirection.Z * _G.YslSettings.SpeedValue
                )
                rootPart.Velocity = velocity
            end
        end)
    end)
end

--// No Clip System
local noClipConnection
local function setupNoClip()
    if noClipConnection then
        noClipConnection:Disconnect()
    end
    
    if not _G.YslSettings.NoClip then
        -- Re-enable collision
        for _, v in pairs(character:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = true
            end
        end
        return
    end
    
    noClipConnection = RunService.Stepped:Connect(function()
        if _G.YslSettings.NoClip then
            for _, v in pairs(character:GetDescendants()) do
                if v:IsA("BasePart") then
                    v.CanCollide = false
                end
            end
        end
    end)
end

--// ULTIMATE Smart Coin Magnet
local magnetConnection
local function setupSmartMagnet()
    if magnetConnection then
        magnetConnection:Disconnect()
    end
    
    if not _G.YslSettings.Magnet then return end
    
    magnetConnection = RunService.Heartbeat:Connect(function()
        if not _G.YslSettings.Magnet or not rootPart then return end
        
        pcall(function()
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") or v:IsA("MeshPart") then
                    local name = v.Name:lower()
                    local parent = v.Parent and v.Parent.Name:lower() or ""
                    
                    -- Check if it's a collectible
                    local isCollectible = false
                    
                    -- Coin/Radio detection
                    if name:find("coin") or name:find("radio") or name:find("money") or 
                       name:find("cash") or name:find("collect") or name:find("pickup") or
                       name:find("token") or parent:find("coin") or parent:find("radio") then
                        isCollectible = true
                    end
                    
                    -- Radioactive glow detection
                    if v:FindFirstChild("PointLight") then
                        local light = v:FindFirstChild("PointLight")
                        if light and (light.Color == Color3.fromRGB(0, 255, 0) or 
                                      light.Color == Color3.fromRGB(0, 255, 127) or
                                      light.Brightness > 2) then
                            isCollectible = true
                        end
                    end
                    
                    -- Particle effect detection (radioactive items usually have particles)
                    if v:FindFirstChildOfClass("ParticleEmitter") then
                        isCollectible = true
                    end
                    
                    -- EXCLUDE players and NPCs
                    if v.Parent and v.Parent:FindFirstChild("Humanoid") then
                        isCollectible = false
                    end
                    
                    -- EXCLUDE your own character
                    if v:IsDescendantOf(character) then
                        isCollectible = false
                    end
                    
                    -- Collect the item
                    if isCollectible then
                        -- Teleport to player
                        v.CFrame = rootPart.CFrame
                        v.CanCollide = false
                        v.Anchored = false
                        
                        -- Force touch event
                        if v.Touched then
                            firetouchinterest(rootPart, v, 0)
                            task.wait(0.01)
                            firetouchinterest(rootPart, v, 1)
                        end
                        
                        _G.YslSettings.CoinsCollected = _G.YslSettings.CoinsCollected + 1
                    end
                end
            end
        end)
        
        task.wait(_G.YslSettings.MagnetSpeed)
    end)
end

--// Auto Collect (Instant teleport to collectibles)
local autoCollectConnection
local function setupAutoCollect()
    if autoCollectConnection then
        autoCollectConnection:Disconnect()
    end
    
    if not _G.YslSettings.AutoCollect then return end
    
    autoCollectConnection = RunService.Heartbeat:Connect(function()
        pcall(function()
            local nearestCoin = nil
            local nearestDistance = math.huge
            
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") then
                    local name = v.Name:lower()
                    if (name:find("coin") or name:find("radio")) and not v:IsDescendantOf(character) then
                        local distance = (v.Position - rootPart.Position).Magnitude
                        if distance < nearestDistance and distance < 200 then
                            nearestDistance = distance
                            nearestCoin = v
                        end
                    end
                end
            end
            
            if nearestCoin then
                rootPart.CFrame = nearestCoin.CFrame
            end
        end)
        task.wait(0.5)
    end)
end

