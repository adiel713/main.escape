--// 8. TELEPORT TAB
TeleportTab:CreateSection("ğŸ¯ Quick Teleports")

TeleportTab:CreateButton({
    Name = "ğŸ Teleport to Win/End Zone",
    Callback = function()
        local found = false
        pcall(function()
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") then
                    local name = v.Name:lower()
                    if name:find("win") or name:find("end") or name:find("finish") or 
                       name:find("safe") or name:find("goal") then
                        rootPart.CFrame = v.CFrame + Vector3.new(0, 5, 0)
                        found = true
                        Rayfield:Notify({
                            Title = "Teleported",
                            Content = "Warped to: " .. v.Name .. " ğŸ",
                            Duration = 3
                        })
                        break
                    end
                end
            end
        end)
        
        if not found then
            Rayfield:Notify({
                Title = "Error",
                Content = "Win zone not found in workspace",
                Duration = 3
            })
        end
    end,
})

TeleportTab:CreateButton({
    Name = "ğŸ“ Teleport to Highest Point",
    Callback = function()
        local highestY = -math.huge
        local safePart = nil
        
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("BasePart") and v.Anchored and v.Position.Y > highestY then
                highestY = v.Position.Y
                safePart = v
            end
        end
        
        if safePart then
            rootPart.CFrame = CFrame.new(safePart.Position + Vector3.new(0, 10, 0))
            Rayfield:Notify({
                Title = "Teleported",
                Content = "Safe spot (Height: " .. math.floor(highestY) .. ") ğŸ“",
                Duration = 3
            })
        end
    end,
})

TeleportTab:CreateButton({
    Name = "ğŸ” Return to Spawn",
    Callback = function()
        local spawn = Workspace:FindFirstChild("SpawnLocation") or 
                      Workspace:FindFirstChild("Spawn") or
                      Workspace:FindFirstChild("Spawns")
        
        if spawn then
            rootPart.CFrame = spawn.CFrame + Vector3.new(0, 5, 0)
        else
            rootPart.CFrame = CFrame.new(0, 50, 0)
        end
        
        Rayfield:Notify({
            Title = "Teleported",
            Content = "Returned to spawn point ğŸ”",
            Duration = 2
        })
    end,
})

TeleportTab:CreateSection("ğŸ’° Event Teleports")

TeleportTab:CreateButton({
    Name = "â˜¢ï¸ Teleport to Radioactive Event",
    Callback = function()
        local found = false
        -- Look for radioactive particles/lights
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("PointLight") then
                if v.Color == Color3.fromRGB(0, 255, 0) or v.Brightness > 3 then
                    if v.Parent and v.Parent:IsA("BasePart") then
                        rootPart.CFrame = v.Parent.CFrame + Vector3.new(0, 10, 0)
                        found = true
                        Rayfield:Notify({
                            Title = "Teleported",
                            Content = "Warped to radioactive event â˜¢ï¸",
                            Duration = 3
                        })
                        break
                    end
                end
            end
        end
        
        if not found then
            Rayfield:Notify({
                Title = "Error",
                Content = "Radioactive event not active",
                Duration = 3
            })
        end
    end,
})

--// 9. VISUAL TAB
VisualTab:CreateSection("ğŸ¨ Visual Enhancements")

VisualTab:CreateToggle({
    Name = "ğŸŒ… Remove Fog",
    CurrentValue = false,
    Flag = "NoFog",
    Callback = function(Value)
        if Value then
            game.Lighting.FogEnd = 100000
            game.Lighting.FogStart = 0
        else
            game.Lighting.FogEnd = 1000
            game.Lighting.FogStart = 0
        end
    end,
})

VisualTab:CreateButton({
    Name = "â˜€ï¸ Full Brightness",
    Callback = function()
        game.Lighting.Brightness = 3
        game.Lighting.ClockTime = 12
        game.Lighting.GlobalShadows = false
        game.Lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
        game.Lighting.Ambient = Color3.fromRGB(255, 255, 255)
        
        Rayfield:Notify({
            Title = "Brightness",
            Content = "Full brightness enabled â˜€ï¸",
            Duration = 2
        })
    end,
})

VisualTab:CreateButton({
    Name = "ğŸŒ™ Night Mode",
    Callback = function()
        game.Lighting.ClockTime = 0
        game.Lighting.Brightness = 1
        
        Rayfield:Notify({
            Title = "Night Mode",
            Content = "Dark mode activated ğŸŒ™",
            Duration = 2
        })
    end,
})

VisualTab:CreateButton({
    Name = "ğŸ§¹ Clean Workspace (Remove Lag)",
    Callback = function()
        local removed = 0
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Smoke") or
               v:IsA("Fire") or v:IsA("Sparkles") then
                v:Destroy()
                removed = removed + 1
            end
        end
        
        Rayfield:Notify({
            Title = "Cleanup",
            Content = "Removed " .. removed .. " visual effects ğŸ§¹",
            Duration = 3
        })
    end,
})

VisualTab:CreateSection("ğŸ¯ ESP Features")

VisualTab:CreateButton({
    Name = "ğŸ’° Highlight All Coins",
    Callback = function()
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("BasePart") then
                local name = v.Name:lower()
                if name:find("coin") or name:find("radio") then
                    local highlight = Instance.new("SelectionBox")
                    highlight.Parent = v
                    highlight.Adornee = v
                    highlight.LineThickness = 0.1
                    highlight.Color3 = Color3.fromRGB(255, 255, 0)
                end
            end
        end
        
        Rayfield:Notify({
            Title = "ESP",
            Content = "All coins highlighted in yellow ğŸ’°",
            Duration = 3
        })
    end,
})

