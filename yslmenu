--// Load Rayfield UI
local success, Rayfield = pcall(function()
    return loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
end)

if not success then
    warn("‚ùå Failed to load Rayfield UI Library")
    return
end

--// Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local UserInputService = game:GetService("UserInputService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

--// Player Variables
local lplayer = Players.LocalPlayer
local character = lplayer.Character or lplayer.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local rootPart = character:WaitForChild("HumanoidRootPart")

--// Settings
_G.YslBrainrot = {
    -- Movement
    Speed = false,
    SpeedValue = 100,
    InfJump = false,
    NoClip = false,
    
    -- Collection
    AutoCollectBrainrots = false,
    AutoCollectMoney = false,
    BrainrotRarity = "All",
    OnlyMutations = false,
    
    -- NEW: UFO Event
    AutoCollectUFOCoins = false,
    UFOMagnetRange = 100,
    AutoCraftUFO = false,
    AutoSpinUFO = false,
    
    -- Radioactive Event
    RadioactiveEvent = false,
    AutoCompleteObby = false,
    AutoCollectRadCoins = false,
    AutoDestroyBombs = false,
    AutoSpinRadioactive = false,
    
    -- Divine/Celestial
    AutoCollectDivineBlocks = false,
    AutoUseDivineBlocks = false,
    
    -- Wave Protection
    WaveImmunity = true,
    AutoHideSafezone = false,
    RemoveWaves = false,
    
    -- VIP Bypass
    VIPBypass = false,
    VIPCheckInterval = 2,
    
    -- Auto Features
    AutoRebirth = false,
    RebirthAt = 300,
    AutoUpgradeSpeed = false,
    AutoUpgradeCarry = false,
    AutoUpgradePlot = false,
    
    -- NEW: Money Features
    InfiniteMoney = false,
    MoneyMultiplier = 1000,
    AutoDepositBrainrots = true,
    
    -- Spawn Machine
    AutoUseSpawnMachine = false,
    
    -- ESP
    BrainrotESP = false,
    SafezoneESP = false,
    RadCoinESP = false,
    UFOCoinESP = false,
    DivineLuckyBlockESP = false,
    
    -- Stats
    Stats = {
        BrainrotsCollected = 0,
        WavesBlocked = 0,
        RadCoinsCollected = 0,
        UFOCoinsCollected = 0,
        ObbyCompletions = 0,
        MoneyGenerated = 0,
        DivineBlocksOpened = 0
    }
}

--// Connections Storage
local Connections = {}

--// Helper Functions
local function Notify(title, content, duration)
    Rayfield:Notify({
        Title = title,
        Content = content,
        Duration = duration or 3,
        Image = 4483362458
    })
end

local function Disconnect(name)
    if Connections[name] then
        Connections[name]:Disconnect()
        Connections[name] = nil
    end
end

local function GetBrainrotFolder()
    return Workspace:FindFirstChild("Brainrots") or 
           Workspace:FindFirstChild("Items") or 
           Workspace:FindFirstChild("BrainrotSpawns") or 
           Workspace
end

local function GetSafeZones()
    local safezones = {}
    for _, v in pairs(Workspace:GetDescendants()) do
        if v:IsA("BasePart") then
            local name = v.Name:lower()
            if name:find("trench") or name:find("safe") or name:find("pit") or name:find("gap") then
                table.insert(safezones, v)
            end
        end
    end
    return safezones
end

local function GetPlayerMoney()
    -- Try multiple methods to get money
    local leaderstats = lplayer:FindFirstChild("leaderstats")
    if leaderstats then
        local money = leaderstats:FindFirstChild("Money") or 
                     leaderstats:FindFirstChild("Cash") or 
                     leaderstats:FindFirstChild("Coins")
        if money then
            return money.Value
        end
    end
    return 0
end

--// WAVE IMMUNITY SYSTEM (Enhanced)
local function SetupWaveImmunity()
    if not _G.YslBrainrot.WaveImmunity then
        Disconnect("WaveImmunity")
        return
    end
    
    Connections.WaveImmunity = RunService.Heartbeat:Connect(function()
        pcall(function()
            if not character or not humanoid or not rootPart then return end
            
            -- Method 1: Health protection
            if humanoid.Health < humanoid.MaxHealth then
                humanoid.Health = humanoid.MaxHealth
                _G.YslBrainrot.Stats.WavesBlocked = _G.YslBrainrot.Stats.WavesBlocked + 1
            end
            
            -- Method 2: Remove water around player (larger area)
            local region = Region3.new(
                rootPart.Position - Vector3.new(50, 50, 50),
                rootPart.Position + Vector3.new(50, 50, 50)
            )
            region = region:ExpandToGrid(4)
            Workspace.Terrain:FillRegion(region, 4, Enum.Material.Air)
            
            -- Method 3: Disable swimming/drowning/ragdoll
            humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, false)
            humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
            humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
            
            -- Method 4: Remove wave parts near player
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") then
                    local name = v.Name:lower()
                    if (name:find("wave") or name:find("tsunami") or name:find("water")) and 
                       (v.Position - rootPart.Position).Magnitude < 100 then
                        v:Destroy()
                    end
                end
            end
        end)
    end)
end

--// VIP BYPASS SYSTEM (Optimized)
local lastVIPCheck = 0
local function SetupVIPBypass()
    if not _G.YslBrainrot.VIPBypass then
        Disconnect("VIPBypass")
        -- Restore VIP doors
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("BasePart") and v:GetAttribute("YslVIPDoor") then
                v.CanCollide = true
                v.Transparency = v:GetAttribute("OriginalTransparency") or 0
            end
        end
        return
    end
    
    Connections.VIPBypass = RunService.Heartbeat:Connect(function()
        local currentTime = tick()
        if currentTime - lastVIPCheck < _G.YslBrainrot.VIPCheckInterval then return end
        lastVIPCheck = currentTime
        
        pcall(function()
            local nearbyDistance = 75
            
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") then
                    local name = v.Name:lower()
                    
                    -- Check for VIP doors/glass/walls
                    if name:find("vip") or name:find("glass") or name:find("door") or 
                       (v.Parent and v.Parent.Name:lower():find("vip")) then
                        local distance = (v.Position - rootPart.Position).Magnitude
                        
                        if distance < nearbyDistance then
                            if not v:GetAttribute("YslVIPDoor") then
                                v:SetAttribute("YslVIPDoor", true)
                                v:SetAttribute("OriginalTransparency", v.Transparency)
                            end
                            
                            v.CanCollide = false
                            v.Transparency = 0.8
                        else
                            if v:GetAttribute("YslVIPDoor") then
                                v.CanCollide = true
                                v.Transparency = v:GetAttribute("OriginalTransparency") or 0
                            end
                        end
                    end
                end
            end
        end)
    end)
end

--// SPEED BYPASS SYSTEM
local function SetupSpeedBypass()
    if not _G.YslBrainrot.Speed then
        Disconnect("Speed")
        if humanoid then humanoid.WalkSpeed = 16 end
        return
    end
    
    Connections.Speed = RunService.Heartbeat:Connect(function()
        pcall(function()
            if not humanoid or not rootPart then return end
            
            humanoid.WalkSpeed = _G.YslBrainrot.SpeedValue
            
            -- CFrame boost
            if humanoid.MoveDirection.Magnitude > 0 then
                local boost = (_G.YslBrainrot.SpeedValue / 16) - 1
                rootPart.CFrame = rootPart.CFrame + (humanoid.MoveDirection * boost * 0.1)
            end
        end)
    end)
end

--// NO CLIP SYSTEM
local function SetupNoClip()
    if not _G.YslBrainrot.NoClip then
        Disconnect("NoClip")
        for _, v in pairs(character:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = true
            end
        end
        return
    end
    
    Connections.NoClip = RunService.Stepped:Connect(function()
        for _, v in pairs(character:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
    end)
end

--// BRAINROT VALIDATION
local function IsBrainrotValid(brainrot)
    if not brainrot or not brainrot:IsA("Model") then return false end
    
    -- Check rarity filter
    if _G.YslBrainrot.BrainrotRarity ~= "All" then
        local brainrotName = brainrot.Name:lower()
        local targetRarity = _G.YslBrainrot.BrainrotRarity:lower()
        
        if not brainrotName:find(targetRarity) then
            return false
        end
    end
    
    -- Check mutation filter
    if _G.YslBrainrot.OnlyMutations then
        local hasMutation = false
        for _, v in pairs(brainrot:GetDescendants()) do
            if v:IsA("ParticleEmitter") or v:IsA("PointLight") or v:IsA("Fire") then
                hasMutation = true
                break
            end
        end
        if not hasMutation then return false end
    end
    
    return true
end

--// BRAINROT AUTO-COLLECTION (Enhanced)
local function SetupAutoCollectBrainrots()
    if not _G.YslBrainrot.AutoCollectBrainrots then
        Disconnect("AutoCollectBrainrots")
        return
    end
    
    Connections.AutoCollectBrainrots = RunService.Heartbeat:Connect(function()
        pcall(function()
            if not rootPart then return end
            
            local brainrotFolder = GetBrainrotFolder()
            
            for _, brainrot in pairs(brainrotFolder:GetChildren()) do
                if IsBrainrotValid(brainrot) then
                    local primaryPart = brainrot.PrimaryPart or 
                                       brainrot:FindFirstChild("HumanoidRootPart") or 
                                       brainrot:FindFirstChildWhichIsA("BasePart")
                    
                    if primaryPart then
                        -- Teleport to player
                        primaryPart.CFrame = rootPart.CFrame
                        primaryPart.CanCollide = false
                        primaryPart.Anchored = false
                        
                        -- Fire touch events
                        if primaryPart.Touched then
                            firetouchinterest(rootPart, primaryPart, 0)
                            task.wait(0.01)
                            firetouchinterest(rootPart, primaryPart, 1)
                        end
                        
                        _G.YslBrainrot.Stats.BrainrotsCollected = _G.YslBrainrot.Stats.BrainrotsCollected + 1
                    end
                end
            end
        end)
        task.wait(0.05)
    end)
end

--// NEW: UFO COIN AUTO-COLLECT (MAGNET SYSTEM)
local function SetupUFOCoinMagnet()
    if not _G.YslBrainrot.AutoCollectUFOCoins then
        Disconnect("UFOCoins")
        return
    end
    
    Connections.UFOCoins = RunService.Heartbeat:Connect(function()
        pcall(function()
            if not rootPart then return end
            
            -- Find UFO coins in workspace
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") or v:IsA("Model") then
                    local name = v.Name:lower()
                    
                    -- Check if it's a UFO coin
                    if name:find("ufo") and (name:find("coin") or name:find("currency")) then
                        local coinPart = v:IsA("Model") and (v.PrimaryPart or v:FindFirstChildWhichIsA("BasePart")) or v
                        
                        if coinPart then
                            local distance = (coinPart.Position - rootPart.Position).Magnitude
                            
                            -- Magnet range check
                            if distance < _G.YslBrainrot.UFOMagnetRange then
                                -- Teleport coin to player
                                coinPart.CFrame = rootPart.CFrame
                                coinPart.CanCollide = false
                                
                                -- Fire touch
                                if coinPart.Touched then
                                    firetouchinterest(rootPart, coinPart, 0)
                                    task.wait(0.01)
                                    firetouchinterest(rootPart, coinPart, 1)
                                end
                                
                                _G.YslBrainrot.Stats.UFOCoinsCollected = _G.YslBrainrot.Stats.UFOCoinsCollected + 1
                            end
                        end
                    end
                end
            end
        end)
        task.wait(0.05)
    end)
end

--// NEW: AUTO-CRAFT UFO BRAINROTS (Black Hole)
local function SetupAutoCraftUFO()
    if not _G.YslBrainrot.AutoCraftUFO then return end
    
    task.spawn(function()
        while _G.YslBrainrot.AutoCraftUFO do
            pcall(function()
                -- Find black hole/UFO crafting station
                local blackHole = Workspace:FindFirstChild("BlackHole") or 
                                 Workspace:FindFirstChild("UFOCrafter") or
                                 Workspace:FindFirstChild("UFOBlackHole")
                
                if blackHole then
                    -- Auto deposit brainrots for crafting
                    local brainrotFolder = GetBrainrotFolder()
                    
                    for _, brainrot in pairs(brainrotFolder:GetChildren()) do
                        if brainrot:IsA("Model") and brainrot.Name:lower():find("celestial") then
                            local part = brainrot.PrimaryPart or brainrot:FindFirstChildWhichIsA("BasePart")
                            if part then
                                -- Move brainrot to black hole
                                part.CFrame = blackHole.CFrame
                                task.wait(0.5)
                            end
                        end
                    end
                end
            end)
            task.wait(5)
        end
    end)
end

--// NEW: AUTO SPIN UFO WHEEL
local function SetupAutoSpinUFO()
    if not _G.YslBrainrot.AutoSpinUFO then return end
    
    task.spawn(function()
        while _G.YslBrainrot.AutoSpinUFO do
            pcall(function()
                -- Find UFO spin wheel
                local ufoWheel = Workspace:FindFirstChild("UFOSpin") or 
                                Workspace:FindFirstChild("UFOSpinWheel") or
                                Workspace:FindFirstChild("UFOWheel")
                
                if ufoWheel then
                    local clickDetector = ufoWheel:FindFirstChildOfClass("ClickDetector", true)
                    if clickDetector then
                        fireclickdetector(clickDetector)
                        task.wait(2)
                    end
                    
                    -- Alternative: Fire remote
                    if ReplicatedStorage:FindFirstChild("UFOSpin") then
                        ReplicatedStorage.UFOSpin:FireServer()
                    end
                end
            end)
            task.wait(3)
        end
    end)
end

--// RADIOACTIVE EVENT - AUTO COMPLETE OBBY
local function FindSecretTunnel()
    for _, v in pairs(Workspace:GetDescendants()) do
        if v:IsA("BasePart") then
            local name = v.Name:lower()
            if (name:find("gate") or name:find("tunnel") or name:find("entrance")) and 
               (name:find("destroy") or name:find("broken") or name:find("secret")) then
                return v
            end
        end
    end
    return nil
end

local function SetupAutoCompleteObby()
    if not _G.YslBrainrot.AutoCompleteObby then return end
    
    task.spawn(function()
        while _G.YslBrainrot.AutoCompleteObby do
            pcall(function()
                -- Check for radioactive event (green lighting)
                if game.Lighting.Ambient == Color3.fromRGB(0, 255, 0) or 
                   game.Lighting.ColorShift_Top == Color3.fromRGB(0, 255, 0) or
                   game.Lighting.OutdoorAmbient == Color3.fromRGB(0, 255, 0) then
                    
                    local tunnel = FindSecretTunnel()
                    if tunnel then
                        -- Teleport to tunnel
                        rootPart.CFrame = tunnel.CFrame + Vector3.new(0, 10, 0)
                        task.wait(2)
                        
                        -- Find end point
                        local endPoint = Workspace:FindFirstChild("ObbyEnd") or 
                                        Workspace:FindFirstChild("Finish") or
                                        Workspace:FindFirstChild("ObbyFinish")
                        
                        if endPoint then
                            rootPart.CFrame = endPoint.CFrame
                            _G.YslBrainrot.Stats.ObbyCompletions = _G.YslBrainrot.Stats.ObbyCompletions + 1
                            Notify("Obby Complete", "Completed radioactive obby! ‚ò¢Ô∏è", 2)
                        end
                    end
                end
            end)
            task.wait(30)
        end
    end)
end

--// RADIOACTIVE COIN AUTO-COLLECT
local function SetupAutoCollectRadCoins()
    if not _G.YslBrainrot.AutoCollectRadCoins then
        Disconnect("RadCoins")
        return
    end
    
    Connections.RadCoins = RunService.Heartbeat:Connect(function()
        pcall(function()
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") or v:IsA("Model") then
                    local name = v.Name:lower()
                    
                    if name:find("radioactive") and (name:find("coin") or name:find("currency")) then
                        local part = v:IsA("Model") and (v.PrimaryPart or v:FindFirstChildWhichIsA("BasePart")) or v
                        
                        if part then
                            -- Check for green glow
                            local hasGreenGlow = false
                            for _, child in pairs((v:IsA("Model") and v or v.Parent):GetDescendants()) do
                                if child:IsA("PointLight") and child.Color == Color3.fromRGB(0, 255, 0) then
                                    hasGreenGlow = true
                                    break
                                end
                            end
                            
                            if hasGreenGlow or name:find("green") then
                                part.CFrame = rootPart.CFrame
                                part.CanCollide = false
                                
                                if part.Touched then
                                    firetouchinterest(rootPart, part, 0)
                                    task.wait(0.01)
                                    firetouchinterest(rootPart, part, 1)
                                end
                                
                                _G.YslBrainrot.Stats.RadCoinsCollected = _G.YslBrainrot.Stats.RadCoinsCollected + 1
                            end
                        end
                    end
                end
            end
        end)
        task.wait(0.05)
    end)
end

--// BOMBARDIRO BOMB AUTO-DESTROY
local function SetupAutoDestroyBombs()
    if not _G.YslBrainrot.AutoDestroyBombs then
        Disconnect("Bombs")
        return
    end
    
    Connections.Bombs = RunService.Heartbeat:Connect(function()
        pcall(function()
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("Model") and (v.Name:lower():find("bomb") or v.Name:lower():find("bombardiro")) then
                    local bombPart = v.PrimaryPart or v:FindFirstChildWhichIsA("BasePart")
                    
                    if bombPart then
                        -- Teleport bomb to player
                        bombPart.CFrame = rootPart.CFrame
                        
                        -- Find and use bat
                        local tool = lplayer.Character:FindFirstChildOfClass("Tool")
                        if not tool or not tool.Name:lower():find("bat") then
                            -- Try to equip bat from backpack
                            for _, item in pairs(lplayer.Backpack:GetChildren()) do
                                if item:IsA("Tool") and item.Name:lower():find("bat") then
                                    humanoid:EquipTool(item)
                                    tool = item
                                    break
                                end
                            end
                        end
                        
                        if tool and tool:FindFirstChild("Handle") then
                            firetouchinterest(tool.Handle, bombPart, 0)
                            task.wait(0.01)
                            firetouchinterest(tool.Handle, bombPart, 1)
                        end
                    end
                end
            end
        end)
        task.wait(0.1)
    end)
end

--// AUTO SPIN RADIOACTIVE WHEEL
local function SetupAutoSpinRadioactive()
    if not _G.YslBrainrot.AutoSpinRadioactive then return end
    
    task.spawn(function()
        while _G.YslBrainrot.AutoSpinRadioactive do
            pcall(function()
                local wheel = Workspace:FindFirstChild("RadioactiveSpin") or 
                             Workspace:FindFirstChild("RadioactiveWheel") or
                             Workspace:FindFirstChild("RadioactiveSpinWheel")
                
                if wheel then
                    local clickDetector = wheel:FindFirstChildOfClass("ClickDetector", true)
                    if clickDetector then
                        fireclickdetector(clickDetector)
                    end
                    
                    -- Try remote
                    if ReplicatedStorage:FindFirstChild("RadioactiveSpin") then
                        ReplicatedStorage.RadioactiveSpin:FireServer()
                    end
                end
            end)
            task.wait(2)
        end
    end)
end

--// NEW: DIVINE LUCKY BLOCK AUTO-FARM
local function SetupAutoDivineBlocks()
    if not _G.YslBrainrot.AutoCollectDivineBlocks then
        Disconnect("DivineBlocks")
        return
    end
    
    Connections.DivineBlocks = RunService.Heartbeat:Connect(function()
        pcall(function()
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("Model") or v:IsA("BasePart") then
                    local name = v.Name:lower()
                    
                    if name:find("divine") and name:find("lucky") then
                        local part = v:IsA("Model") and (v.PrimaryPart or v:FindFirstChildWhichIsA("BasePart")) or v
                        
                        if part then
                            -- Teleport block to player
                            part.CFrame = rootPart.CFrame
                            part.CanCollide = false
                            
                            if part.Touched then
                                firetouchinterest(rootPart, part, 0)
                                task.wait(0.01)
                                firetouchinterest(rootPart, part, 1)
                            end
                            
                            -- Auto-use if enabled
                            if _G.YslBrainrot.AutoUseDivineBlocks then
                                task.wait(0.5)
                                
                                -- Find in inventory and use
                                local divineBlock = lplayer.Character:FindFirstChild("DivineLuckyBlock") or
                                                   lplayer.Backpack:FindFirstChild("DivineLuckyBlock")
                                
                                if divineBlock then
                                    if divineBlock.Parent == lplayer.Backpack then
                                        humanoid:EquipTool(divineBlock)
                                        task.wait(0.3)
                                    end
                                    
                                    divineBlock:Activate()
                                    _G.YslBrainrot.Stats.DivineBlocksOpened = _G.YslBrainrot.Stats.DivineBlocksOpened + 1
                                end
                            end
                        end
                    end
                end
            end
        end)
        task.wait(0.1)
    end)
end

--// NEW: INFINITE MONEY SYSTEM
local function SetupInfiniteMoney()
    if not _G.YslBrainrot.InfiniteMoney then
        Disconnect("InfiniteMoney")
        return
    end
    
    Connections.InfiniteMoney = RunService.Heartbeat:Connect(function()
        pcall(function()
            -- Find money leaderstats
            local leaderstats = lplayer:FindFirstChild("leaderstats")
            if leaderstats then
                local money = leaderstats:FindFirstChild("Money") or 
                             leaderstats:FindFirstChild("Cash") or 
                             leaderstats:FindFirstChild("Coins")
                
                if money and money:IsA("IntValue") or money:IsA("NumberValue") then
                    -- Set to high value
                    money.Value = money.Value + (_G.YslBrainrot.MoneyMultiplier * 100)
                    _G.YslBrainrot.Stats.MoneyGenerated = _G.YslBrainrot.Stats.MoneyGenerated + (_G.YslBrainrot.MoneyMultiplier * 100)
                end
            end
            
            -- Alternative: Fire money remote
            if ReplicatedStorage:FindFirstChild("AddMoney") then
                ReplicatedStorage.AddMoney:FireServer(_G.YslBrainrot.MoneyMultiplier * 100)
            end
        end)
        task.wait(1)
    end)
end

--// AUTO DEPOSIT BRAINROTS
local function SetupAutoDepositBrainrots()
    if not _G.YslBrainrot.AutoDepositBrainrots then return end
    
    task.spawn(function()
        while _G.YslBrainrot.AutoDepositBrainrots do
            pcall(function()
                -- Find base/deposit zone
                local base = Workspace:FindFirstChild("Base") or 
                            Workspace:FindFirstChild("SafeZone") or
                            Workspace:FindFirstChild("Spawn")
                
                if base then
                    -- Check if player has brainrots
                    local hasBrainrots = false
                    for _, item in pairs(lplayer.Character:GetChildren()) do
                        if item:IsA("Model") and item.Name:lower():find("brainrot") then
                            hasBrainrots = true
                            break
                        end
                    end
                    
                    if hasBrainrots then
                        -- Teleport to base
                        local originalPos = rootPart.CFrame
                        rootPart.CFrame = base.CFrame + Vector3.new(0, 5, 0)
                        task.wait(1)
                        
                        -- Return to original position
                        rootPart.CFrame = originalPos
                    end
                end
            end)
            task.wait(10)
        end
    end)
end

--// AUTO REBIRTH SYSTEM
local function GetPlayerSpeed()
    local speedStat = lplayer:FindFirstChild("leaderstats") and lplayer.leaderstats:FindFirstChild("Speed")
    if speedStat then
        return speedStat.Value
    end
    return humanoid.WalkSpeed
end

local function PerformRebirth()
    pcall(function()
        -- Try multiple rebirth methods
        local rebirthButton = Workspace:FindFirstChild("RebirthButton")
        
        if rebirthButton and rebirthButton:FindFirstChildOfClass("ClickDetector") then
            fireclickdetector(rebirthButton:FindFirstChildOfClass("ClickDetector"))
        end
        
        -- Try remote
        if ReplicatedStorage:FindFirstChild("Rebirth") then
            ReplicatedStorage.Rebirth:FireServer()
        elseif ReplicatedStorage:FindFirstChild("RebirthEvent") then
            ReplicatedStorage.RebirthEvent:FireServer()
        end
    end)
end

local function SetupAutoRebirth()
    if not _G.YslBrainrot.AutoRebirth then return end
    
    task.spawn(function()
        while _G.YslBrainrot.AutoRebirth do
            pcall(function()
                local currentSpeed = GetPlayerSpeed()
                if currentSpeed >= _G.YslBrainrot.RebirthAt then
                    PerformRebirth()
                    Notify("Auto Rebirth", "Rebirthed at " .. currentSpeed .. " speed! üîÑ", 3)
                    task.wait(10)
                end
            end)
            task.wait(5)
        end
    end)
end

--// AUTO UPGRADE SYSTEMS
local function SetupAutoUpgradeSpeed()
    if not _G.YslBrainrot.AutoUpgradeSpeed then return end
    
    task.spawn(function()
        while _G.YslBrainrot.AutoUpgradeSpeed do
            pcall(function()
                local speedShop = Workspace:FindFirstChild("SpeedShop") or
                                 Workspace:FindFirstChild("SpeedUpgrade")
                
                if speedShop then
                    local button = speedShop:FindFirstChild("BuyButton") or
                                  speedShop:FindFirstChild("UpgradeButton")
                    
                    if button and button:FindFirstChildOfClass("ClickDetector") then
                        fireclickdetector(button:FindFirstChildOfClass("ClickDetector"))
                    end
                end
                
                -- Try remote
                if ReplicatedStorage:FindFirstChild("UpgradeSpeed") then
                    ReplicatedStorage.UpgradeSpeed:FireServer()
                end
            end)
            task.wait(2)
        end
    end)
end

