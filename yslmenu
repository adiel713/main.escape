print("ğŸŒŠ Loading Ultimate Bypass Script...")
wait(1)

-- Load Rayfield
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")
local VirtualUser = game:GetService("VirtualUser")

-- Player
local Player = Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
local Humanoid = Character:WaitForChild("Humanoid")

-- Settings
local Settings = {
    -- Auto Farm
    AutoFarm = false,
    AutoCollect = false,
    AutoPlace = false,
    InstantPickup = true,
    BypassSafeZone = true,
    
    -- Upgrades
    AutoSpeed = false,
    AutoCapacity = false,
    AutoIncome = false,
    AutoRebirth = false,
    
    -- Bypass
    DeleteWaves = false,
    NoClip = false,
    InfJump = false,
    
    -- Player
    WalkSpeed = false,
    WalkSpeedValue = 150,
    JumpPower = false,
    JumpPowerValue = 150,
    
    -- Visual
    ESP = false,
    
    -- Other
    AntiAFK = true,
}

-- Game Remotes (will be found)
local Remotes = {
    Pickup = {},
    Place = {},
    Upgrade = {},
    Rebirth = {},
    Collect = {},
}

-- State
local Loops = {}
local ESPObjects = {}

-- Window
local Window = Rayfield:CreateWindow({
    Name = "ğŸŒŠ Escape Tsunami - Bypass Edition",
    LoadingTitle = "Deep Searching Game...",
    LoadingSubtitle = "Finding All Remotes",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "EscapeTsunamiBypass",
        FileName = "Config"
    },
})

-- Tabs
local MainTab = Window:CreateTab("ğŸ¯ Auto Farm", 4483362458)
local BypassTab = Window:CreateTab("ğŸ”“ Bypass", 4483362458)
local UpgradeTab = Window:CreateTab("âš¡ Upgrades", 4483362458)
local PlayerTab = Window:CreateTab("ğŸ‘¤ Player", 4483362458)
local VisualTab = Window:CreateTab("ğŸ‘ï¸ Visuals", 4483362458)
local InfoTab = Window:CreateTab("â„¹ï¸ Info", 4483362458)

-- Notify
local function Notify(title, content, duration)
    Rayfield:Notify({
        Title = title,
        Content = content,
        Duration = duration or 3,
    })
end

--[[ DEEP REMOTE SEARCH ]]

local function DeepSearchRemotes()
    print("ğŸ” DEEP SEARCHING ALL REMOTES...")
    Notify("Searching", "Deep searching game remotes...", 5)
    
    local function searchContainer(container, containerName)
        for _, obj in pairs(container:GetDescendants()) do
            if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
                local name = obj.Name:lower()
                local fullPath = obj:GetFullName()
                
                print("ğŸ“ Found Remote:", obj.Name, "at", fullPath)
                
                -- Pickup/Collect remotes
                if name:find("pick") or name:find("collect") or name:find("grab") or 
                   name:find("touch") or name:find("get") then
                    table.insert(Remotes.Pickup, obj)
                    print("âœ… PICKUP Remote:", fullPath)
                end
                
                -- Place/Deposit remotes
                if name:find("place") or name:find("deposit") or name:find("put") or 
                   name:find("drop") or name:find("sell") then
                    table.insert(Remotes.Place, obj)
                    print("âœ… PLACE Remote:", fullPath)
                end
                
                -- Upgrade remotes
                if name:find("upgrade") or name:find("buy") or name:find("purchase") or 
                   name:find("speed") or name:find("capacity") or name:find("income") or
                   name:find("multiplier") then
                    table.insert(Remotes.Upgrade, obj)
                    print("âœ… UPGRADE Remote:", fullPath)
                end
                
                -- Rebirth remotes
                if name:find("rebirth") or name:find("prestige") or name:find("reset") then
                    table.insert(Remotes.Rebirth, obj)
                    print("âœ… REBIRTH Remote:", fullPath)
                end
                
                -- Collect money remotes
                if (name:find("claim") or name:find("collect")) and 
                   (name:find("money") or name:find("cash") or name:find("income")) then
                    table.insert(Remotes.Collect, obj)
                    print("âœ… COLLECT Remote:", fullPath)
                end
            end
        end
    end
    
    -- Search everywhere
    searchContainer(ReplicatedStorage, "ReplicatedStorage")
    searchContainer(Workspace, "Workspace")
    
    if Player:FindFirstChild("PlayerScripts") then
        searchContainer(Player.PlayerScripts, "PlayerScripts")
    end
    
    if Player:FindFirstChild("PlayerGui") then
        searchContainer(Player.PlayerGui, "PlayerGui")
    end
    
    print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
    print("FOUND:")
    print("Pickup Remotes:", #Remotes.Pickup)
    print("Place Remotes:", #Remotes.Place)
    print("Upgrade Remotes:", #Remotes.Upgrade)
    print("Rebirth Remotes:", #Remotes.Rebirth)
    print("Collect Remotes:", #Remotes.Collect)
    print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
    
    Notify("Search Complete!", "Found " .. (#Remotes.Pickup + #Remotes.Place + #Remotes.Upgrade) .. " remotes", 5)
end

--[[ HELPER FUNCTIONS ]]

local function UpdateChar()
    Character = Player.Character
    if Character then
        HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart")
        Humanoid = Character:FindFirstChild("Humanoid")
    end
end

local function TP(pos)
    if HumanoidRootPart then
        HumanoidRootPart.CFrame = CFrame.new(pos)
    end
end

--[[ WAVE/TSUNAMI DELETION ]]

local function DeleteWaves()
    pcall(function()
        -- Search for waves/tsunamis
        for _, obj in pairs(Workspace:GetDescendants()) do
            local name = obj.Name:lower()
            if name:find("wave") or name:find("tsunami") or name:find("water") then
                if obj:IsA("BasePart") or obj:IsA("Model") then
                    obj:Destroy()
                end
            end
        end
    end)
end

local function DeleteWavesLoop()
    if Loops.DeleteWaves then return end
    
    Loops.DeleteWaves = RunService.Heartbeat:Connect(function()
        if Settings.DeleteWaves then
            DeleteWaves()
        end
    end)
end

--[[ GET ITEMS ]]

local function GetBrainrots()
    local items = {}
    
    -- Search in Workspace
    for _, obj in pairs(Workspace:GetDescendants()) do
        local name = obj.Name:lower()
        if name:find("brainrot") or name:find("item") or name:find("collectible") or
           obj:GetAttribute("Rarity") or obj:GetAttribute("Class") then
            
            local pos
            if obj:IsA("Model") then
                pos = obj:GetPivot().Position
            elseif obj:IsA("BasePart") then
                pos = obj.Position
            end
            
            if pos and obj.Parent then
                table.insert(items, {
                    Object = obj,
                    Position = pos,
                    Name = obj.Name
                })
            end
        end
    end
    
    return items
end

local function GetLuckyBlocks()
    local blocks = {}
    
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj.Name:lower():find("lucky") and obj:IsA("BasePart") then
            table.insert(blocks, {
                Object = obj,
                Position = obj.Position
            })
        end
    end
    
    return blocks
end

--[[ INSTANT PICKUP WITH BYPASS ]]

local function InstantPickup(item)
    if not item or not item.Object then return end
    
    pcall(function()
        local obj = item.Object
        
        if Settings.BypassSafeZone then
            -- Method 1: Direct teleport and fire all pickup remotes
            TP(item.Position)
            wait(0.05)
            
            -- Find part to touch
            local touchPart = obj
            if obj:IsA("Model") then
                touchPart = obj:FindFirstChildWhichIsA("BasePart") or obj.PrimaryPart
            end
            
            if touchPart and touchPart:IsA("BasePart") then
                -- Fire touch
                firetouchinterest(HumanoidRootPart, touchPart, 0)
                wait(0.01)
                firetouchinterest(HumanoidRootPart, touchPart, 1)
                wait(0.05)
            end
            
            -- Try all pickup remotes
            for _, remote in pairs(Remotes.Pickup) do
                pcall(function()
                    if remote:IsA("RemoteEvent") then
                        remote:FireServer(obj)
                        remote:FireServer(obj.Name)
                        remote:FireServer()
                    else
                        remote:InvokeServer(obj)
                        remote:InvokeServer(obj.Name)
                        remote:InvokeServer()
                    end
                end)
            end
            
            -- Try clicking detector if exists
            if obj:FindFirstChildWhichIsA("ClickDetector") then
                fireclickdetector(obj:FindFirstChildWhichIsA("ClickDetector"))
            end
            
            if touchPart and touchPart:FindFirstChildWhichIsA("ClickDetector") then
                fireclickdetector(touchPart:FindFirstChildWhichIsA("ClickDetector"))
            end
            
        else
            -- Normal pickup
            TP(item.Position)
            wait(0.1)
            
            local touchPart = obj:IsA("BasePart") and obj or obj:FindFirstChildWhichIsA("BasePart")
            if touchPart then
                firetouchinterest(HumanoidRootPart, touchPart, 0)
                wait(0.05)
                firetouchinterest(HumanoidRootPart, touchPart, 1)
            end
        end
    end)
end

--[[ AUTO PLACE ]]

local function AutoPlace()
    pcall(function()
        -- Get all tools in backpack and character
        local tools = {}
        
        if Player:FindFirstChild("Backpack") then
            for _, tool in pairs(Player.Backpack:GetChildren()) do
                if tool:IsA("Tool") then
                    table.insert(tools, tool)
                end
            end
        end
        
        if Character then
            for _, tool in pairs(Character:GetChildren()) do
                if tool:IsA("Tool") then
                    table.insert(tools, tool)
                end
            end
        end
        
        -- Place each tool
        for _, tool in pairs(tools) do
            -- Equip
            Humanoid:EquipTool(tool)
            wait(0.05)
            
            -- Try all place remotes
            for _, remote in pairs(Remotes.Place) do
                pcall(function()
                    if remote:IsA("RemoteEvent") then
                        remote:FireServer(tool)
                        remote:FireServer(tool.Name)
                        remote:FireServer()
                    else
                        remote:InvokeServer(tool)
                        remote:InvokeServer(tool.Name)
                        remote:InvokeServer()
                    end
                end)
            end
            
            wait(0.05)
            
            -- Unequip
            Humanoid:UnequipTools()
            wait(0.05)
        end
    end)
end

--[[ UPGRADES ]]

local function BuyUpgrade(upgradeType)
    pcall(function()
        for _, remote in pairs(Remotes.Upgrade) do
            local name = remote.Name:lower()
            
            -- Try to match upgrade type
            if upgradeType == "speed" and name:find("speed") then
                if remote:IsA("RemoteEvent") then
                    remote:FireServer()
                    remote:FireServer("Speed")
                    remote:FireServer(1)
                else
                    remote:InvokeServer()
                    remote:InvokeServer("Speed")
                    remote:InvokeServer(1)
                end
            elseif upgradeType == "capacity" and (name:find("capacity") or name:find("carry")) then
                if remote:IsA("RemoteEvent") then
                    remote:FireServer()
                    remote:FireServer("Capacity")
                    remote:FireServer(1)
                else
                    remote:InvokeServer()
                    remote:InvokeServer("Capacity")
                    remote:InvokeServer(1)
                end
            elseif upgradeType == "income" and (name:find("income") or name:find("multiplier")) then
                if remote:IsA("RemoteEvent") then
                    remote:FireServer()
                    remote:FireServer("Income")
                    remote:FireServer(1)
                else
                    remote:InvokeServer()
                    remote:InvokeServer("Income")
                    remote:InvokeServer(1)
                end
            end
        end
        
        -- Also try generic upgrade remotes with parameters
        for _, remote in pairs(Remotes.Upgrade) do
            pcall(function()
                if remote:IsA("RemoteEvent") then
                    remote:FireServer(upgradeType)
                    remote:FireServer(upgradeType:upper())
                    remote:FireServer(upgradeType:lower())
                else
                    remote:InvokeServer(upgradeType)
                    remote:InvokeServer(upgradeType:upper())
                    remote:InvokeServer(upgradeType:lower())
                end
            end)
        end
    end)
end

local function DoRebirth()
    pcall(function()
        for _, remote in pairs(Remotes.Rebirth) do
            if remote:IsA("RemoteEvent") then
                remote:FireServer()
            else
                remote:InvokeServer()
            end
        end
        Notify("Rebirth", "Attempted rebirth!", 2)
    end)
end

--[[ COLLECT MONEY ]]

local function CollectMoney()
    pcall(function()
        for _, remote in pairs(Remotes.Collect) do
            if remote:IsA("RemoteEvent") then
                remote:FireServer()
            else
                remote:InvokeServer()
            end
        end
    end)
end

--[[ ESP ]]

local function CreateESP(obj, text, color)
    if not obj or not obj:IsA("BasePart") then return end
    if obj:FindFirstChild("ESP_GUI") then return end
    
    pcall(function()
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "ESP_GUI"
        billboard.Adornee = obj
        billboard.Size = UDim2.new(0, 100, 0, 40)
        billboard.StudsOffset = Vector3.new(0, 2, 0)
        billboard.AlwaysOnTop = true
        billboard.Parent = obj
        
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 1, 0)
        frame.BackgroundColor3 = color or Color3.fromRGB(255, 0, 255)
        frame.BackgroundTransparency = 0.3
        frame.Parent = billboard
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 8)
        corner.Parent = frame
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = text or "Item"
        label.TextColor3 = Color3.white
        label.TextStrokeTransparency = 0
        label.TextScaled = true
        label.Font = Enum.Font.GothamBold
        label.Parent = frame
        
        table.insert(ESPObjects, billboard)
    end)
end

local function ClearESP()
    for _, esp in pairs(ESPObjects) do
        pcall(function() esp:Destroy() end)
    end
    ESPObjects = {}
end

--[[ AUTO FARM LOOP ]]

local function StartAutoFarm()
    if Loops.Farm then return end
    
    Notify("Auto Farm", "Started!", 3)
    
    Loops.Farm = RunService.Heartbeat:Connect(function()
        if not Settings.AutoFarm then return end
        
        pcall(function()
            UpdateChar()
            if not HumanoidRootPart then return end
            
            -- Apply mods
            if Settings.WalkSpeed and Humanoid then
                Humanoid.WalkSpeed = Settings.WalkSpeedValue
            end
            
            if Settings.JumpPower and Humanoid then
                Humanoid.JumpPower = Settings.JumpPowerValue
            end
            
            if Settings.InfJump and Humanoid then
                Humanoid.JumpPower = 150
                Humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping, true)
            end
            
            -- NoClip
            if Settings.NoClip then
                for _, part in pairs(Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
            
            -- Auto collect
            if Settings.AutoCollect then
                local brainrots = GetBrainrots()
                local luckyBlocks = GetLuckyBlocks()
                
                -- Collect brainrots
                for i, item in ipairs(brainrots) do
                    if not Settings.AutoFarm then break end
                    
                    if Settings.ESP then
                        local part = item.Object:IsA("BasePart") and item.Object or item.Object:FindFirstChildWhichIsA("BasePart")
                        if part then
                            CreateESP(part, "Brainrot", Color3.fromRGB(255, 0, 255))
                        end
                    end
                    
                    InstantPickup(item)
                    wait(0.1)
                    
                    if i >= 3 then break end
                end
                
                -- Collect lucky blocks
                for i, block in ipairs(luckyBlocks) do
                    if not Settings.AutoFarm then break end
                    
                    if Settings.ESP then
                        CreateESP(block.Object, "Lucky Block", Color3.fromRGB(255, 255, 0))
                    end
                    
                    InstantPickup(block)
                    wait(0.1)
                    
                    if i >= 2 then break end
                end
            end
            
            -- Auto place
            if Settings.AutoPlace then
                AutoPlace()
            end
            
            -- Collect money
            CollectMoney()
            
            -- Auto upgrades
            if Settings.AutoSpeed then
                BuyUpgrade("speed")
            end
            
            if Settings.AutoCapacity then
                BuyUpgrade("capacity")
            end
            
            if Settings.AutoIncome then
                BuyUpgrade("income")
            end
            
            if Settings.AutoRebirth then
                DoRebirth()
            end
        end)
        
        wait(0.3)
    end)
end

local function StopAutoFarm()
    if Loops.Farm then
        Loops.Farm:Disconnect()
        Loops.Farm = nil
        ClearESP()
        Notify("Auto Farm", "Stopped!", 3)
    end
end

