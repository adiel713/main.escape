    Callback = function()
        local removed = 0
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("BasePart") and v.CanCollide then
                if v.Name:lower():find("wall") or v.Name:lower():find("barrier") then
                    v:Destroy()
                    removed = removed + 1
                end
            end
        end
        Notify("Cleanup", "Removed " .. removed .. " barriers üóëÔ∏è", 3)
    end,
})

